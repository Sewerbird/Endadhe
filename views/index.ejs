		<script type="text/javascript" src="/js/eventer.js"></script>
		<script type="text/javascript" src="/js/torrentBar.js"></script>
		<script type="text/javascript">
			var game = generateInitialState();
			var cameraView = new CameraView(canvas, game);
			//Handle browser input
			document.getElementById("canvas").focus();
			var simTimer = setInterval(function(){
				simulateGameState(game,game,0.1);
				cameraView.render();
			},100)
		</script>
	</body>
